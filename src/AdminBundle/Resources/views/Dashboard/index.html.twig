{% extends "AdminBundle::admin.html.twig" %}


{% block body %}

    <div class="col-md-12">
        <img class="logofirstpage" src="{{ asset('img/logo.png') }}">
    </div>

    
    <div class="stickers1">

        <div class="sticker1">
            Nb Participants <p class="numbers">{{ participants|length }}</p>
        </div>

        <div class="sticker2">
            Nb Sessions <p class="numbers">{{ sessions|length }}</p>
        </div>

    </div>
    <div class="stickers2">
        <div class="sticker3">
            Nb Questions <p class="numbers">{{ questions|length }}</p>
        </div>

        <div class="sticker4">
            Nb Questionnaires <p class="numbers">{{ questionnaires|length }}</p>
        </div>
    </div>


    <div class="canvas">
        <canvas id="myChart"></canvas>

    </div>


    <script>
        new Chart(document.getElementById("myChart"), {
            "type":"doughnut",
                "data":{"labels":["-50","+=50"],
                    "datasets":[{"label":"chart",
                    "data":[{{ m50 }},{{ p50 }}],
                    "backgroundColor":["rgb(255, 99, 132)", "rgb(54, 162, 235)"]
                    }]
                }
            });
    </script>

{% endblock %}